    <name>GoogleAnalytics.ChartMacro</name>
    <number>0</number>
    <className>XWiki.WikiMacroClass</className>
    <guid>c01b65a3-0f86-4011-9a81-96660af36544</guid>
    <property>
      <code>{{velocity}}
{{googleanalytics clientid="$!{xcontext.macro.params.clientid}" serviceaccount="$!{xcontext.macro.params.serviceaccount}"}}
/**
   * Create a new DataChart instance with the given query parameters
   * and Google chart options. It will be rendered inside an element
   * with the id "chart-container".
   */
  var dataChart = new gapi.analytics.googleCharts.DataChart({
    query: {
      ids: '${xcontext.macro.params.ids}',
      metrics: '${xcontext.macro.params.metrics}',
      dimensions: '${xcontext.macro.params.dimensions}',
      'start-date': '${xcontext.macro.params.start-date}',
      'end-date': '${xcontext.macro.params.end-date}',
      'max-results': ${xcontext.macro.params.max-results}
      #if($xcontext.macro.params.sort &amp;&amp; $xcontext.macro.params.sort!="")
      ,'sort' : '${xcontext.macro.params.sort}'
      #end
    },
    chart: {
      container: 'chart-containerGACOUNT',
      type: '${xcontext.macro.params.type}',
      options: {
        width: '${xcontext.macro.params.width}'
      }
    }
  });
  dataChart.execute();
{{/googleanalytics}}
{{/velocity}}</code>
